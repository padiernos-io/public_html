{#
/**
 * @file
 * Default theme implementation to display a paragraph.
 *
 * Available variables:
 * - paragraph: Full paragraph entity.
 *   Only method names starting with "get", "has", or "is" and a few common
 *   methods such as "id", "label", and "bundle" are available. For example:
 *   - paragraph.getCreatedTime() will return the paragraph creation timestamp.
 *   - paragraph.id(): The paragraph ID.
 *   - paragraph.bundle(): The type of the paragraph, for example, "image" or "text".
 *   - paragraph.getOwnerId(): The user ID of the paragraph author.
 *   See Drupal\paragraphs\Entity\Paragraph for a full list of public properties
 *   and methods for the paragraph object.
 * - content: All paragraph items. Use {{ content }} to print them all,
 *   or print a subset such as {{ content.field_example }}. Use
 *   {{ content|without('field_example') }} to temporarily suppress the printing
 *   of a given child element.
 * - attributes: HTML attributes for the containing element.
 *   The attributes.class element may contain one or more of the following
 *   classes:
 *   - paragraphs: The current template type (also known as a "theming hook").
 *   - paragraphs--type-[type]: The current paragraphs type. For example, if the paragraph is an
 *     "Image" it would result in "paragraphs--type--image". Note that the machine
 *     name will often be in a short form of the human readable label.
 *   - paragraphs--view-mode--[view_mode]: The View Mode of the paragraph; for example, a
 *     preview would result in: "paragraphs--view-mode--preview", and
 *     default: "paragraphs--view-mode--default".
 * - view_mode: View mode; for example, "preview" or "full".
 * - logged_in: Flag for authenticated user status. Will be true when the
 *   current user is a logged-in member.
 * - is_admin: Flag for admin user status. Will be true when the current user
 *   is an administrator.
 *
 * @see template_preprocess_paragraph()
 *
 * @ingroup themeable
 */
#}
{%
  set classes = [
  'paragraph',
  'ept-paragraph',
  'ept-basic-button',
  'ept-cta',
  content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.styles,
  'image-position-' ~ content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.image_position,
  'align-content-' ~ content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.align_content,
  'paragraph-id-' ~ paragraph.id(),
]
%}

{% if content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.add_nofollow is not empty and content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.add_nofollow == 1 %}
  {% set nofollow = ' rel="nofollow"' %}
{% else %}
  {% set nofollow = '' %}
{% endif %}

{% if content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.open_in_new_tab is not empty and content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.open_in_new_tab == 1 %}
  {% set target = ' target="_blank"' %}
{% else %}
  {% set target = '' %}
{% endif %}

{% if content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.alignment == 'left' %}
  {% set classes = classes|merge(['ept-align-left']) %}
{% elseif content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.alignment == 'center' %}
  {% set classes = classes|merge(['ept-align-center']) %}
{% elseif content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.alignment == 'right' %}
  {% set classes = classes|merge(['ept-align-right']) %}
{% endif %}

{% if content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.shape == 'square' %}
  {% set classes = classes|merge(['ept-shape-square']) %}
{% elseif content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.shape == 'round' %}
  {% set classes = classes|merge(['ept-shape-round']) %}
{% elseif content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.shape == 'circle' %}
  {% set classes = classes|merge(['ept-shape-circle']) %}
{% endif %}


{% if content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.size == 'small' %}
  {% set classes = classes|merge(['ept-size-small']) %}
{% elseif content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.size == 'medium' %}
  {% set classes = classes|merge(['ept-size-medium']) %}
{% elseif content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.size == 'large' %}
  {% set classes = classes|merge(['ept-size-large']) %}
{% endif %}

{% if content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.stetched is not empty and content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.stetched == 1 %}
  {% set classes = classes|merge(['ept-stretched']) %}
{% endif %}

{% if content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.stretched is not empty and content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.stretched == 1 %}
  {% set classes = classes|merge(['ept-stretched']) %}
{% endif %}

{% if content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.custom_class_name is not empty %}
  {% set button_custom_classes = content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.custom_class_name %}
{% endif %}

{% if content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.link_options2.add_nofollow is not empty and content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.link_options2.add_nofollow == 1 %}
  {% set button2_nofollow = ' rel="nofollow"' %}
{% else %}
  {% set button2_nofollow = '' %}
{% endif %}

{% if content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.link_options2.open_in_new_tab is not empty and content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.link_options2.open_in_new_tab == 1 %}
  {% set button2_target = ' target="_blank"' %}
{% else %}
  {% set button2_target = '' %}
{% endif %}

{%
  set button2_classes = []
%}

{% if content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.link_options2.alignment %}
  {% set button2_classes = button2_classes|merge(['ept-align-' ~ content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.link_options2.alignment]) %}
{% endif %}

{% if content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.link_options2.shape %}
  {% set button2_classes = button2_classes|merge(['ept-shape-' ~ content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.link_options2.shape]) %}
{% endif %}


{% if content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.link_options2.size %}
  {% set button2_classes = button2_classes|merge(['ept-size-' ~ content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.link_options2.size]) %}
{% endif %}

{% if content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.link_options2.stetched is not empty and content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.link_options2.stetched == 1 %}
  {% set button2_classes = button2_classes|merge(['ept-stretched']) %}
{% endif %}

{% if content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.link_options2.stretched is not empty and content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.link_options2.stretched == 1 %}
  {% set button2_classes = button2_classes|merge(['ept-stretched']) %}
{% endif %}

{% if content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.link_options2.custom_class_name is not empty %}
  {% set button2_custom_classes = content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.link_options2.custom_class_name %}
{% endif %}

{{ attach_library('ept_basic_button/ept_basic_button_view') }}
{{ attach_library('ept_cta/ept_cta') }}
<div{{ attributes.addClass(classes) }}>
  <div class="bg-inner"></div>
  <div class="ept-container">
    {% if content.field_ept_title|render %}
      {% if content.field_ept_settings['#object'].field_ept_settings.ept_settings.title_options.title_wrapper is empty %}
        <h2>
      {% elseif content.field_ept_settings['#object'].field_ept_settings.ept_settings.title_options.title_wrapper == 'none' %}

      {% elseif content.field_ept_settings['#object'].field_ept_settings.ept_settings.title_options.title_wrapper in ['h1', 'h2', 'h3', 'h4', 'h5'] %}
        <{{ content.field_ept_settings['#object'].field_ept_settings.ept_settings.title_options.title_wrapper }}>
      {% else %}
        <h2>
      {% endif %}

      {% if content.field_ept_settings['#object'].field_ept_settings.ept_settings.title_options.strip_tags and content.field_ept_settings['#object'].field_ept_settings.ept_settings.title_options.strip_tags == 1 %}
        {{ content.field_ept_title|render|striptags('<span><br><i><img><svg>')|trim|raw }}
      {% else %}
        {{ content.field_ept_title }}
      {% endif %}

      {% if content.field_ept_settings['#object'].field_ept_settings.ept_settings.title_options.title_wrapper is empty %}
        </h2>
      {% elseif content.field_ept_settings['#object'].field_ept_settings.ept_settings.title_options.title_wrapper == 'none' %}
      {% elseif content.field_ept_settings['#object'].field_ept_settings.ept_settings.title_options.title_wrapper in ['h1', 'h2', 'h3', 'h4', 'h5'] %}
        </{{ content.field_ept_settings['#object'].field_ept_settings.ept_settings.title_options.title_wrapper }}>
      {% else %}
        </h2>
      {% endif %}
    {% endif %}
    {% block content %}
      <div class="ept-paragraph-cta__content">
        {% if content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.styles == 'two_columns' or content.field_ept_settings['#object'].field_ept_settings.0.ept_settings.styles == 'two_columns_fluid' %}
          <div class="column-1">
            {{ content.field_ept_cta_column_image }}
          </div>
          <div class="column-2">
            {{ content|without('field_ept_settings', 'field_ept_cta_column_image', 'field_ept_cta_link', 'field_ept_title', 'field_ept_cta_second_link') }}
            <div class="cta-button-wrapper">
              <span class="ept-basic-button-wrapper">
                <a href="{{ content.field_ept_cta_link.0['#url'] }}" class="ept-basic-button {{ button_custom_classes }}" {{ nofollow|raw }} {{ target|raw }}>
                  {{ content.field_ept_cta_link.0['#title'] }}
                </a>
              </span>
              {% if content.field_ept_cta_second_link|render %}
                <span class="cta-button-wrapper ept-button2-wrapper {{ button2_classes|join(' ') }}">
                <a href="{{ content.field_ept_cta_second_link.0['#url'] }}" class="ept-basic-button2 {{ button2_classes|join(' ') }}" {{ button2_nofollow|raw }} {{ button2_target|raw }}>
                  {{ content.field_ept_cta_second_link.0['#title'] }}
                </a>
              </span>
              {% endif %}
            </div>
          </div>
        {% else %}
          {{ content|without('field_ept_settings', 'field_ept_cta_link', 'field_ept_title', 'field_ept_cta_second_link') }}
          <div class="cta-button-wrapper">
            <span class="ept-basic-button-wrapper">
              <a href="{{ content.field_ept_cta_link.0['#url'] }}" class="ept-basic-button {{ button_custom_classes }}" {{ nofollow|raw }} {{ target|raw }}>
                {{ content.field_ept_cta_link.0['#title'] }}
              </a>
            </span>
            {% if content.field_ept_cta_second_link|render %}
              <span class="cta-button-wrapper ept-button2-wrapper {{ button2_classes|join(' ') }}">
                <a href="{{ content.field_ept_cta_second_link.0['#url'] }}" class="ept-basic-button2 {{ button2_classes|join(' ') }}" {{ button2_nofollow|raw }} {{ button2_target|raw }}>
                  {{ content.field_ept_cta_second_link.0['#title'] }}
                </a>
              </span>
            {% endif %}
          </div>
        {% endif %}
      </div>
    {% endblock %}
  </div>
</div>
{{ styles|raw }}
{{ button_styles|raw }}
{{ cta_styles|raw }}
